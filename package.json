{
  "name": "p-seeder-base",
  "version": "0.0.1",
  "description": "P-Seeder base library shared between P-Seeder CLI and P-Seeder App",
  "main": "dist/index.js",
  "scripts": {
    "dev": "nodemon -w src --exec \"babel --plugins transform-regenerator src -d dist\"",
    "test": "ava",
    "format": "standard src/**.js --fix",
    "lint": "eslint src",
    "build": "babel src -s -D -d dist --presets es2015,stage-0",
    "start": "node dist",
    "install": "yarn && npm run build"
  },
  "repository": {
    "type": "git",
    "url": "https://gitlab.dldl.fr/p-project/p-seeder-base.git"
  },
  "keywords": [
    "P-Seeder"
  ],
  "author": "P-Project",
  "license": "MIT",
  "dependencies": {
    "babel-plugin-transform-runtime": "^6.16.0",
    "body-parser": "^1.15.2",
    "deepmerge": "^1.3.2",
    "express": "^4.14.0",
    "express-validation": "^1.0.1",
    "joi": "^10.2.0",
    "request": "^2.81.0",
    "request-promise": "^4.1.1",
    "webtorrent-hybrid": "^1.0.5",
    "ws": "^2.2.3",
    "yamljs": "^0.2.9"
  },
  "devDependencies": {
    "ava": "^0.19.0",
    "babel-cli": "^6.9.0",
    "babel-core": "^6.9.0",
    "babel-polyfill": "^6.16.0",
    "babel-preset-es2015": "^6.9.0",
    "babel-preset-stage-0": "^6.5.0",
    "babel-register": "^6.18.0",
    "eslint": "^3.9.1",
    "eslint-config-standard": "^6.2.1",
    "eslint-plugin-promise": "^3.3.0",
    "eslint-plugin-standard": "^2.0.1",
    "nodemon": "^1.11.0",
    "standard": "^10.0.0",
    "supertest": "^2.0.1"
  },
  "bin": {
    "pseederd": "dist/index.js"
  },
  "ava": {
    "files": [
      "src/tests/*.js",
      "!src/tests/index.js"
    ],
    "source": [
      "src/**.js"
    ],
    "require": [
      "babel-polyfill",
      "babel-register"
    ],
    "babel": "inherit"
  }
}
